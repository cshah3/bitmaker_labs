<%= form_for(@restaurant) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :cuisine %>
    <%= f.text_field :cuisine %>

    <%= f.label :url %>
    <%= f.text_field :url %>

    <%= f.submit %>
<% end %>